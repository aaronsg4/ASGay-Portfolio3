        function determine() {
            var userinputone = $("#userinputone").val();
            var userinputtwo = $("#userinputtwo").val();
            var userinputthree = $("#userinputthree").val();
            var userinputfour = $("#userinputfour").val();
            var userinputfive = $("#userinputfive").val();
            var first = parseInt(userinputone);
            var second = parseInt(userinputtwo);
            var third = parseInt(userinputthree);
            var fourth = parseInt(userinputfour);
            var fifth = parseInt(userinputfive);
            var A = Math.max(first, second, third, fourth, fifth);
            var B = Math.min(first, second, third, fourth, fifth);
            var C = (first + second + third + fourth + fifth) / 5;
            var D = first + second + third + fourth + fifth;
            var E = first * second * third * fourth * fifth;
            $("#emptyparagraphone").html(A);
            $("#emptyparagraphtwo").html(B);
            $("#emptyparagraphthree").html(C);
            $("#emptyparagraphfour").html(D);
            $("#emptyparagraphfive").html(E);
        }
           
