    <script>
        function palindrome() {

            var A = $("#userinputPalindrome").val();
            var Anospace = A.replace(/[\W_]/g, "");
            var Asmall = Anospace.toLowerCase();

            var B = Asmall.length;
            var C = "";

            for (i = B - 1; i >= 0; i--) {

                C = C + Asmall.charAt(i);
            }

            if (C == Asmall) {
                document.getElementById('blank').innerHTML = "The word is a palindrome!"
                document.getElementById('blank2').innerHTML = "***" + C + " IS equal to " + Asmall + "."
            }
            else {
                document.getElementById('blank').innerHTML = "The word is not a palindrome!"
                document.getElementById('blank2').innerHTML = "***" + C + " is NOT equal to " + Asmall + "."
            }

        }
